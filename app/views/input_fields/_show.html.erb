<div id="input_fields">
  <% input_fields.each do |input_field| %>
    <p>
        <strong>Type:</strong>
         <%= input_field.type %>
    </p>
    <p>
        <strong>Survey:</strong>
        <%= input_field.survey_id %>
    </p>
    <div>
        <%= button_to "Destroy this Input Field", survey_input_field_url(input_field.survey, input_field), method: :delete, class: "btn btn-gradient" %>
    </div>
  <% end %>
</div>